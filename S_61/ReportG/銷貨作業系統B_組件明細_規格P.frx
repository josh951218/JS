    private void Text6_BeforePrint(object sender, EventArgs e)
    {                                                               
      string date  = ((String)Report.GetColumnValue(&quot;Data_.sale_sadate&quot;));
      string date1 = ((String)Report.GetColumnValue(&quot;Data_.sale_sadate1&quot;));
      if(((String)Report.GetParameterValue(&quot;民國或西元&quot;)) == &quot;1&quot;)
      {                                                       
        Text6.Text = &quot;銷貨日期:&quot; + Substring(date,0,3) + &quot;/&quot; + Substring(date,3,2)+ &quot;/&quot; + Substring(date,5,2) ;
      } 
      else
           Text6.Text = &quot;銷貨日期:&quot; + Substring(date1,0,4) + &quot;/&quot; + Substring(date1,4,2)+ &quot;/&quot; + Substring(date1,6,2) ;
    }
