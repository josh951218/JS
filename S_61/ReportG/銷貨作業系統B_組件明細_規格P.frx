    private void Text11_BeforePrint(object sender, EventArgs e)
    { 
      Text11.Text = &quot;頁    次:&quot;+((Int32)Report.GetVariableValue(&quot;Page&quot;)).ToString() + &quot;/&quot; +((Int32)Report.GetVariableValue(&quot;TotalPages&quot;)).ToString();
    }
    
    private void Text1_BeforePrint(object sender, EventArgs e)
    {
        if(((String)Report.GetParameterValue(&quot;列印抬頭&quot;)) == &quot;True&quot;)   Text1.Visible = true;
        else   Text1.Visible = false;
    }

    private void Text34_BeforePrint(object sender, EventArgs e)
    {
      //除以０會出錯
      string BomQty=&quot;&quot;;
      if(((Decimal)Report.GetColumnValue(&quot;Data_.salebom_itpareprs&quot;)) &gt; 0)
        BomQty = ToString( ((Decimal)Report.GetColumnValue(&quot;Data_.salebom_itqty&quot;)) *  ((Decimal)Report.GetColumnValue(&quot;Data_.salebom_itpkgqty&quot;)) / ((Decimal)Report.GetColumnValue(&quot;Data_.salebom_itpareprs&quot;))  )  ;
      else
        BomQty = ToString( ((Decimal)Report.GetColumnValue(&quot;Data_.salebom_itqty&quot;)) *  ((Decimal)Report.GetColumnValue(&quot;Data_.salebom_itpkgqty&quot;))   )  ;
       
      
      string formate = &quot;f&quot;+ ((String)Report.GetParameterValue(&quot;庫存數量小數&quot;)) ;
      Text34.Text = Decimal.Parse(BomQty).ToString(formate)    ;
    }

    private void Data1_BeforePrint(object sender, EventArgs e)
    {    
      if(((String)Report.GetColumnValue(&quot;Data_.salebom_itno&quot;)) == &quot;&quot;||((String)Report.GetColumnValue(&quot;Data_.salebom_itno&quot;)) == null)
      {
        Data1.Visible = false;  
      }
      else
        Data1.Visible = true;
    }
    

    private void Text33_BeforePrint(object sender, EventArgs e)
    {
      //是否列印備註說明
      if(((String)Report.GetParameterValue(&quot;列印備註說明&quot;)) == &quot;True&quot;) 
