      } 
      else
        Text24.Text =  date1 ;      
    }
    

    private void Text130_BeforePrint(object sender, EventArgs e)
    {
      if (((Int32)Report.GetVariableValue(&quot;Page&quot;))==((Int32)Report.GetVariableValue(&quot;TotalPages&quot;)))
      {
