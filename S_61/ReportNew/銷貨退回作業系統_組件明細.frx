      {Text72.Text=&quot;&quot;;} 
      
      string formet = &quot;f&quot;+((String)Report.GetParameterValue(&quot;庫存數量小數&quot;));
      decimal itqty = ((Decimal)Report.GetColumnValue(&quot;RSALE_.rsalebom_標準用量&quot;));
      decimal itpareprs = ((Decimal)Report.GetColumnValue(&quot;RSALE_.rsalebom_母件比率&quot;));
      decimal itpkgqty = ((Decimal)Report.GetColumnValue(&quot;RSALE_.rsalebom_包裝數量&quot;));
      if(itpareprs == 0)
      {
        Text72.Text=(0M).ToString(formet);
      }
      else
      {
